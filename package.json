{
  "name": "discoverio",
  "description": "Discover something new",
  "repository": "lucky7id/discoverio",
  "version": "0.1.0",
  "scripts": {
      "dev": "webpack-dev-server --config ./webpack.config.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/client",
